<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
             xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools"
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             tools:context=".fragment.VideoDataFragment">
    <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent" android:background="@color/fragment_bg">

        <com.google.android.material.card.MaterialCardView
                android:layout_width="0dp"
                android:layout_height="0dp" app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintHeight_percent="0.9"
                app:cardCornerRadius="30dp" app:layout_constraintWidth_percent="0.9"
                app:cardElevation="1dp" app:layout_constraintHorizontal_bias="0.487"
                app:layout_constraintVertical_bias="0.493">
            <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" android:id="@+id/vdata_layout">

                <EditText
                        android:layout_width="0dp"
                        android:layout_height="0dp" android:id="@+id/et_frag_data_title"
                        android:textSize="20sp"
                        app:layout_constraintLeft_toLeftOf="@id/guideline_vdata_v_left"
                        app:layout_constraintRight_toRightOf="@id/guideline_vdata_v_right"
                        app:layout_constraintTop_toTopOf="@id/guideline_vdata_h_top"
                        app:layout_constraintBottom_toBottomOf="@id/guideline_vdata_h_2"
                        app:layout_constraintVertical_bias="0.0" android:textStyle="bold"
                        android:textColor="@color/black" tools:ignore="SpeakableTextPresentCheck"
                        android:enabled="false" android:backgroundTint="@android:color/transparent"/>

                <TextView
                        android:layout_width="wrap_content"
                        android:id="@+id/tv_frag_data_date"
                        android:textSize="20sp"
                        android:layout_height="wrap_content"
                        app:layout_constraintTop_toTopOf="@id/guideline_vdata_h_5"
                        app:layout_constraintBottom_toBottomOf="@id/guideline_vdata_h_bot"
                        app:layout_constraintLeft_toLeftOf="@id/guideline_vdata_v_left"
                        app:layout_constraintRight_toRightOf="@id/guideline_vdata_v_right"
                        app:layout_constraintHorizontal_bias="0.0"/>
                <TextView
                        android:layout_width="wrap_content"
                        android:id="@+id/tv_frag_data_date_content"
                        android:textSize="20sp"
                        android:layout_height="wrap_content"
                        app:layout_constraintTop_toTopOf="@id/guideline_vdata_h_5"
                        app:layout_constraintBottom_toBottomOf="@id/guideline_vdata_h_bot"
                        app:layout_constraintRight_toRightOf="@id/guideline_vdata_v_right"
                        app:layout_constraintLeft_toRightOf="@id/tv_frag_data_date"
                        app:layout_constraintHorizontal_bias="0.0"/>
                <com.google.android.material.button.MaterialButton
                        android:text="@string/data"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" android:id="@+id/btn_frag_data_player1"
                        app:layout_constraintLeft_toLeftOf="@id/guideline_vdata_v_2"
                        app:layout_constraintRight_toRightOf="@id/guideline_vdata_p1_end"
                        app:layout_constraintTop_toTopOf="@id/guideline_vdata_h_2"
                        app:layout_constraintBottom_toBottomOf="@id/guideline_vdata_h_3" android:gravity="center"
                        android:textStyle="bold" android:backgroundTint="@color/sugar_light_blue"
                        android:textColor="@color/white"
                        android:minWidth="0dp"
                        app:layout_constraintHorizontal_bias="0.3"/>
                <com.google.android.material.button.MaterialButton
                        android:text="@string/data"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" android:id="@+id/btn_frag_data_player2"
                        app:layout_constraintTop_toTopOf="@id/guideline_vdata_h_2"
                        app:layout_constraintBottom_toBottomOf="@id/guideline_vdata_h_3"
                        app:layout_constraintRight_toRightOf="@id/guideline_vdata_v_5"
                        app:layout_constraintLeft_toLeftOf="@id/guideline_vdata_p2_start"
                        android:gravity="center" android:textStyle="bold"
                        android:backgroundTint="@color/sugar_light_red" android:textColor="@color/white"
                        android:minWidth="0dp" app:layout_constraintHorizontal_bias="0.8"/>

                <Button
                        android:layout_width="0dp"
                        android:layout_height="0dp" android:id="@+id/btn_left_circle_1"
                        android:background="@drawable/ic_baseline_circle"
                        tools:ignore="SpeakableTextPresentCheck"
                        app:layout_constraintTop_toTopOf="@id/guideline_vdata_h_2"
                        app:layout_constraintBottom_toBottomOf="@id/guideline_vdata_top_circle2"
                        app:layout_constraintLeft_toLeftOf="@id/guideline_vdata_v_3"
                        app:layout_constraintRight_toRightOf="@id/guideline_vdata_v_center"
                        app:layout_constraintDimensionRatio="1:1" android:clickable="false"
                        android:textColor="@color/white" android:textSize="16sp" android:textStyle="bold"/>
                <Button
                        android:layout_width="0dp"
                        android:layout_height="0dp" android:id="@+id/btn_left_circle_2"
                        android:background="@drawable/ic_baseline_circle"
                        app:layout_constraintTop_toTopOf="@id/guideline_vdata_top_circle2"
                        app:layout_constraintBottom_toBottomOf="@id/guideline_vdata_top_circle3"
                        app:layout_constraintLeft_toLeftOf="@id/guideline_vdata_v_3"
                        app:layout_constraintRight_toRightOf="@id/guideline_vdata_v_center"
                        app:layout_constraintDimensionRatio="1:1" android:clickable="false"
                        android:textColor="@color/white" android:textSize="16sp" android:textStyle="bold"/>
                <Button
                        android:layout_width="0dp"
                        android:layout_height="0dp" android:id="@+id/btn_left_circle_3"
                        android:background="@drawable/ic_baseline_circle"
                        tools:ignore="SpeakableTextPresentCheck"
                        app:layout_constraintTop_toTopOf="@id/guideline_vdata_top_circle3"
                        app:layout_constraintBottom_toBottomOf="@id/guideline_vdata_h_3"
                        app:layout_constraintLeft_toLeftOf="@id/guideline_vdata_v_3"
                        app:layout_constraintRight_toRightOf="@id/guideline_vdata_v_center"
                        app:layout_constraintDimensionRatio="1:1" android:clickable="false"
                        android:textColor="@color/white" android:textSize="16sp" android:textStyle="bold"/>
                <Button
                        android:layout_width="0dp"
                        android:layout_height="0dp" android:id="@+id/btn_right_circle_1"
                        android:background="@drawable/ic_baseline_circle"
                        tools:ignore="SpeakableTextPresentCheck"
                        app:layout_constraintTop_toTopOf="@id/guideline_vdata_h_2"
                        app:layout_constraintBottom_toBottomOf="@id/guideline_vdata_top_circle2"
                        app:layout_constraintLeft_toLeftOf="@id/guideline_vdata_v_center"
                        app:layout_constraintRight_toRightOf="@id/guideline_vdata_v_4"
                        app:layout_constraintDimensionRatio="1:1" android:clickable="false"
                        android:textColor="@color/white" android:textSize="16sp" android:textStyle="bold"/>
                <Button
                        android:layout_width="0dp"
                        android:layout_height="0dp" android:id="@+id/btn_right_circle_2"
                        android:background="@drawable/ic_baseline_circle"
                        tools:ignore="SpeakableTextPresentCheck"
                        app:layout_constraintTop_toTopOf="@id/guideline_vdata_top_circle2"
                        app:layout_constraintBottom_toBottomOf="@id/guideline_vdata_top_circle3"
                        app:layout_constraintLeft_toLeftOf="@id/guideline_vdata_v_center"
                        app:layout_constraintRight_toRightOf="@id/guideline_vdata_v_4"
                        app:layout_constraintDimensionRatio="1:1" android:clickable="false"
                        android:textColor="@color/white" android:textSize="16sp" android:textStyle="bold">

                </Button>
                <Button
                        android:layout_width="0dp"
                        android:layout_height="0dp" android:id="@+id/btn_right_circle_3"
                        android:background="@drawable/ic_baseline_circle"
                        tools:ignore="SpeakableTextPresentCheck"
                        app:layout_constraintTop_toTopOf="@id/guideline_vdata_top_circle3"
                        app:layout_constraintBottom_toBottomOf="@id/guideline_vdata_h_3"
                        app:layout_constraintLeft_toLeftOf="@id/guideline_vdata_v_center"
                        app:layout_constraintRight_toRightOf="@id/guideline_vdata_v_4"
                        app:layout_constraintDimensionRatio="1:1" android:clickable="false"
                        android:textColor="@color/white" android:textSize="16sp" android:textStyle="bold"/>

                <androidx.constraintlayout.widget.Guideline android:layout_width="wrap_content"
                                                            android:layout_height="wrap_content"
                                                            android:id="@+id/guideline_vdata_v_left"
                                                            android:orientation="vertical"
                                                            app:layout_constraintGuide_percent="0.05"/>
                <androidx.constraintlayout.widget.Guideline
                        android:orientation="vertical"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/guideline_vdata_v_right" app:layout_constraintGuide_percent="0.95"/>
                <androidx.constraintlayout.widget.Guideline
                        android:orientation="vertical"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/guideline_vdata_v_2" app:layout_constraintGuide_percent="0.1"/>
                <androidx.constraintlayout.widget.Guideline
                        android:orientation="vertical"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/guideline_vdata_v_3" app:layout_constraintGuide_percent="0.35"/>
                <androidx.constraintlayout.widget.Guideline
                        android:orientation="vertical"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/guideline_vdata_p1_end" app:layout_constraintGuide_percent="0.35"/>
                <androidx.constraintlayout.widget.Guideline
                        android:orientation="vertical"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/guideline_vdata_v_4" app:layout_constraintGuide_percent="0.65"/>
                <androidx.constraintlayout.widget.Guideline
                        android:orientation="vertical"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/guideline_vdata_p2_start" app:layout_constraintGuide_percent="0.65"/>
                <androidx.constraintlayout.widget.Guideline
                        android:orientation="vertical"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/guideline_vdata_v_5" app:layout_constraintGuide_percent="0.9"/>
                <androidx.constraintlayout.widget.Guideline
                        android:orientation="vertical"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/guideline_vdata_v_center" app:layout_constraintGuide_percent="0.5"/>
                <androidx.constraintlayout.widget.Guideline
                        android:orientation="horizontal"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/guideline_vdata_h_top"
                        app:layout_constraintGuide_percent="0.05"/>
                <androidx.constraintlayout.widget.Guideline
                        android:orientation="horizontal"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/guideline_vdata_h_2"
                        app:layout_constraintGuide_percent="0.3"/>
                <androidx.constraintlayout.widget.Guideline
                        android:orientation="horizontal"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/guideline_vdata_h_3"
                        app:layout_constraintGuide_percent="0.8"/>
                <androidx.constraintlayout.widget.Guideline
                        android:orientation="horizontal"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/guideline_vdata_h_4"
                        app:layout_constraintGuide_percent="0.85"/>
                <androidx.constraintlayout.widget.Guideline
                        android:orientation="horizontal"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/guideline_vdata_h_5"
                        app:layout_constraintGuide_percent="0.9"/>
                <androidx.constraintlayout.widget.Guideline
                        android:orientation="horizontal"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/guideline_vdata_h_bot"
                        app:layout_constraintGuide_percent="0.95"/>
                <androidx.constraintlayout.widget.Guideline
                        android:orientation="horizontal"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/guideline_vdata_top_circle2"
                        app:layout_constraintGuide_percent="0.47"/>
                <androidx.constraintlayout.widget.Guideline
                        android:orientation="horizontal"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/guideline_vdata_top_circle3"
                        app:layout_constraintGuide_percent="0.64"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
        </com.google.android.material.card.MaterialCardView>
    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>