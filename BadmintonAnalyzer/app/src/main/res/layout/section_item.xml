<?xml version="1.0" encoding="utf-8"?>
<com.google.android.material.card.MaterialCardView
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:clickable="true"
        android:focusable="true"
        app:strokeColor="@color/egg_yellow"
        app:strokeWidth="5dp"
        android:elevation="0dp"
        android:layout_marginBottom="3dp" android:layout_marginTop="3dp"
        android:backgroundTint="@color/fragment_bg"
        app:cardCornerRadius="20dp"
        android:layout_margin="20dp">


    <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/sectText"
            android:fontFamily="@font/taipei_regular" android:textSize="30sp" android:gravity="center"
            android:text="3333" android:padding="5dp"
            android:textColor="@color/sectionItem" android:textStyle="bold">

    </TextView>
</com.google.android.material.card.MaterialCardView>